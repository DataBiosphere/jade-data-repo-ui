  "scripts": {
    "start": "node tools/start",
    "build": "node tools/build",
    "build:serve": "npm run build && npm run serve",
    "analyze": "NODE_ENV=production webpack --config config/webpack.config.js --profile --json > webpack.stats.json",
    "lint": "eslint  --ext .js --ext .jsx src config test tools",
    "lint:styles": "stylelint 'src/**/*.{js,jsx}'",
    "test": "exit 0",
    "test:coverage": "jest --bail --coverage",
    "test:watch": "jest --watch --verbose",
    "test:resnapshot": "jest --updateSnapshot --verbose",
    "cy:run": "cypress run --record --key adbe5dd9-10e2-42aa-b679-da6d31ae5d51",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test serve http://local.broadinstitute.org:3000 cy:run",
    "test:e2e:dev": "start-server-and-test start http://local.broadinstitute.org:3000 cy:open",
    "format": "prettier \"**/*.{js,jsx}\" --write",
    "validate": "npm run lint && npm run lint:styles && npm run test:coverage && npm run test:e2e:run && npm run bundlesize",
    "setup": "npm install && npm run validate",
    "serve": "serve --listen 3000 build",
    "deploy": "node tools deploy",
    "publish": "node tools publish",
    "bundlesize": "bundlesize",
    "rolldown": "node --inspect tools/migrate rolldown"
  },
  "husky": {
    "hooks": {
      "post-merge": "npm install"
    }
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "bundlesize": [
    {
      "path": "./build/scripts/bundle.*.js",
      "maxSize": "180 kB"
    },
    {
      "path": "./build/scripts/modernizr.*.js",
      "maxSize": "25 kB"
    }
  ]
